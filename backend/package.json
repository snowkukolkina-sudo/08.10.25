{
  "name": "dandy-pos-backend",
  "version": "1.0.0",
  "description": "DANDY POS Backend - Микросервисная архитектура для кассовой системы",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "start:sqlite": "node src/index-sqlite.js",
    "dev": "nodemon src/index.js",
    "dev:sqlite": "nodemon src/index-sqlite.js",
    "workers": "node src/workers/startWorkers.js",
    "dev:workers": "nodemon src/workers/startWorkers.js",
    "test": "jest",
    "migrate": "knex migrate:latest",
    "migrate:sqlite": "knex migrate:latest --env sqlite",
    "seed": "knex seed:run",
    "seed:sqlite": "knex seed:run --env sqlite",
    "test:db": "node test-database.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:restart": "docker-compose restart"
  },
  "dependencies": {
    "amqplib": "^0.10.3",
    "apollo-server-express": "^3.12.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.0.1",
    "graphql": "^16.8.1",
    "graphql-tools": "^9.0.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "sharp": "^0.32.6",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "selfsigned": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "pos",
    "restaurant",
    "microservices",
    "nodejs",
    "postgresql",
    "rabbitmq",
    "graphql"
  ],
  "author": "DANDY Team",
  "license": "MIT"
}
